{
  "app_name": "receiptify",
  "build_files": [
    "print-server.py",
    "requirements.txt"
  ],
  "install_files": [
    {
      "file": "print-server.py",
      "from": "cwd"
    },
    {
      "file": "trim-tool.py",
      "from": "cwd"
    },
    {
      "file": "requirements.txt",
      "from": "cwd"
    },
    {
      "file": "venv",
      "from": "build"
    }
  ],
  "build_script": "scripts/build.sh",
  "systemd": true,
  "exec": "/opt/qp_apps/receiptify/venv/bin/python /opt/qp_apps/receiptify/print-server.py"
}